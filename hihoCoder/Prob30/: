typedef long long LL;

const int MAXN = 100000 + 5;
int n;
int a[MAXN];
bool check(vector<int> &vec) {
    for (int i = 1; i <= n - 1; i++) {
        if (i == 1) {
            vec[i] = a[i] - vec[i - 1];
        }
    }
}
void solve() {
    vector<int> x(n), y(n);
    x[1] = 0;
}
int main() {
